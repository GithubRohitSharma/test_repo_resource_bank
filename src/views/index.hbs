    <!-- Custom JavaScript -->
    <script>
        // Populate academics menu
        const academics_menu = document.getElementById("academics_menu");
        fetch('/get-academics-sem-list', {
            method: 'GET'
        })
        .then(response => response.json())
        .then(data => {
            data.forEach(item => {
                const element = document.createElement("span");
                element.classList.add("dropdown-item");
                element.classList.add("click_cursor");
                element.setAttribute("onclick", `redirectToSemester('${item.name}')`)
                element.innerHTML = `${item.name}`;
                academics_menu.appendChild(element);
            });
        })
        .catch(error => console.error('Error fetching academics menu:', error));

        function redirectToSemester(semester) {
            document.getElementById("semNumH").value = semester;
            let form = document.getElementById("semesterFormH");
            form.setAttribute("action", "/semester");
            form.submit();
        }

        // Tab switching functionality
        function switchTab(tabName) {
            // Remove active class from all buttons
            document.querySelectorAll('.tab-btn').forEach(btn => btn.classList.remove('active'));
            // Add active class to clicked button
            event.target.closest('.tab-btn').classList.add('active');
            
            // Hide all tab content
            document.querySelectorAll('.tab-pane').forEach(pane => pane.style.display = 'none');
            // Show selected tab content
            document.getElementById(tabName + '-content').style.display = 'block';
        }

        // Faculty loading and display
        function fetchFacultyAndRender() {
            fetch('/get-faculty-list')
                .then(response => response.json())
                .then(data => {
                    const teachers = document.getElementById('teachers');
                    teachers.innerHTML = '';
                    
                    data.forEach((faculty, index) => {
                        const facultyCard = document.createElement('div');
                        facultyCard.className = 'col-lg-3 col-md-6';
                        facultyCard.innerHTML = `
                            <div class="card h-100 border-0 shadow-sm">
                                <div class="card-img-top position-relative overflow-hidden" style="height: 200px;">
                                    <img src="${faculty.thumbnailLink}" class="img-fluid w-100 h-100" style="object-fit: cover;" alt="${faculty.properties.facultyName}">
                                </div>
                                <div class="card-body text-center">
                                    <h5 class="card-title mb-1">${faculty.properties.facultyName}</h5>
                                    <p class="text-muted small mb-3">${faculty.properties.facultyRole}</p>
                                    <button class="btn btn-outline-primary btn-sm" onclick="toggleContact('contact-${index}')">
                                        <i class="fas fa-envelope me-1"></i>Contact Info
                                    </button>
                                    <div id="contact-${index}" class="contact-info mt-2" style="display: none;">
                                        <small class="d-block text-muted">
                                            <i class="fas fa-envelope me-1"></i>${faculty.name}
                                        </small>
                                        <small class="d-block text-muted">
                                            <i class="fas fa-phone me-1"></i>${faculty.properties.facultyContact}
                                        </small>
                                        <a href="${faculty.properties.facultyProfile}" target="_blank" class="btn btn-sm btn-primary mt-1">
                                            <i class="fas fa-user me-1"></i>Profile
                                        </a>
                                    </div>
                                </div>
                            </div>`;
                        teachers.appendChild(facultyCard);
                    });
                })
                .catch(error => {
                    console.error('Error fetching faculty:', error);
                    document.getElementById('teachers').innerHTML = '<div class="col-12 text-center text-danger">Error loading faculty information</div>';
                });
        }

        // Schedule loading and display
        function fetchScheduleAndRender() {
            fetch('/get-schedule-list')
                .then(response => response.json())
                .then(data => {
                    const schedulesList = document.getElementById('schedulesList');
                    schedulesList.innerHTML = '';
                    
                    data.forEach(schedule => {
                        const scheduleCard = document.createElement('div');
                        scheduleCard.className = 'col-lg-4 col-md-6';
                        scheduleCard.innerHTML = `
                            <div class="card h-100 border-0 shadow-sm">
                                <div class="card-img-top position-relative">
                                    <img src="${schedule.thumbnailLink}" class="img-fluid w-100" style="cursor: pointer;" onclick="showImageModal('${schedule.thumbnailLink}')" alt="${schedule.name}">
                                </div>
                                <div class="card-body text-center">
                                    <h5 class="card-title">${schedule.name}</h5>
                                    <button class="btn btn-primary btn-sm" onclick="showImageModal('${schedule.thumbnailLink}')">
                                        <i class="fas fa-expand me-1"></i>View Full Size
                                    </button>
                                </div>
                            </div>`;
                        schedulesList.appendChild(scheduleCard);
                    });
                })
                .catch(error => {
                    console.error('Error fetching schedules:', error);
                    document.getElementById('schedulesList').innerHTML = '<div class="col-12 text-center text-danger">Error loading schedules</div>';
                });
        }

        // Toggle contact information
        function toggleContact(contactId) {
            const contact = document.getElementById(contactId);
            contact.style.display = contact.style.display === 'none' ? 'block' : 'none';
        }

        // Show image in modal
        function showImageModal(src) {
            const modal = new bootstrap.Modal(document.getElementById('myModel'));
            document.getElementById('img01').src = src;
            modal.show();
        }

        // Initialize page
        document.addEventListener('DOMContentLoaded', function() {
            fetchFacultyAndRender();
            fetchScheduleAndRender();
        });
    </script>
</body>

</html>
